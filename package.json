{
  "name": "sadko-nuxt",
  "version": "0.0.1",
  "description": "sadko",
  "author": "sadko",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:designer": "nuxt dev --cwd apps/designer",
    "build:designer": "nuxt build --cwd apps/Designer",
