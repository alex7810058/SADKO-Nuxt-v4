  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "nuxt-electron": "^0.7.0"
  },
  "packageManager": "pnpm@10.26.1"
}
